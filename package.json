{
  "name": "react-native-horizontal-timeline",
  "version": "0.2.0",
  "description": "A horizontal scrollable timeline with month days",
  "main": "index.js",
  "scripts": {
    "jest": "./node_modules/.bin/jest --coverage --collectCoverageFrom=*.js",
    "jest:watch": "npm run jest -- --watch",
    "lint": "./node_modules/.bin/eslint ./*.js",
    "format": "./node_modules/.bin/prettier --write {,__tests__}*.js",
    "test": "npm run lint && npm run jest"
  },
  "keywords": [
    "react-native",
    "react-component",
    "react-native-component",
    "react",
    "mobile",
    "ios",
    "android",
    "ui",
    "timeline",
    "horizontal",
    "history"
  ],
  "author": {
    "name": "Carlos Alberto de Araujo Barreto",
    "email": "araujobarret@gmail.com"
  },
  "homepage": "https://github.com/araujobarret/react-native-horizontal-timeline",
  "bugs": {
    "url": "https://github.com/araujobarret/react-native-horizontal-timeline/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/araujobarret/react-native-horizontal-timeline.git"
  },
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-react-native": "^4.0.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.0.0",
    "jest-react-native": "^18.0.0",
    "jest-serializer-enzyme": "^1.0.0",
    "prettier": "^1.16.4",
    "react": "16.3.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-native": "0.55.4",
    "react-test-renderer": "^16.3.1"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "/__tests__/.*\\.test\\.jsx?$",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-navigation|react-native-fabric)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/Examples/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/Examples/"
    ],
    "setupFiles": [
      "<rootDir>setup/setupEnzyme.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/setup/transformAssets.js"
    }
  }
}
